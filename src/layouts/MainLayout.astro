---
import Footer from "../components/Footer.astro";
import Nav from "../components/Nav.astro";
import "./../assets/global.css";
const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>{title}</title>
  </head>
  <body>
    <Nav />

    <slot />

    <Footer />
  </body>
</html>
